fn fizz_buzz(n) {
  fn helper(i) {
    if i < n + 1 {
      let word = if 0 == i % 15 {
        "FizzBuzz~"
      } else if 0 == i % 3 {
        "Fizz~"
      } else if 0 == i % 5 {
        "Buzz~"
      } else {
        ""
      };
      word + helper(i + 1)
    } else {
        ""
    }
  };
  helper(1)
};

fizz_buzz(15)
